# Copy to .env and fill in values. Do not commit .env.

# Google Sign-in (login): redirect URI = http://localhost:5000/api/auth/google/callback
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Google Business Profile (Add Business): redirect URI = http://localhost:5000/api/integrations/google/callback
GOOGLE_GBP_CLIENT_ID=
GOOGLE_GBP_CLIENT_SECRET=
# Optional: or use GOOGLE_GBP_REDIRECT_URI or GOOGLE_REDIRECT_URI_GBP=
# GOOGLE_REDIRECT_URI_GBP=http://localhost:5000/api/integrations/google/callback

BASE_URL=http://localhost:5000

# Required for Google Business Profile (encrypts refresh tokens). Use 32+ chars or 64 hex. Generate: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
INTEGRATION_ENCRYPTION_KEY=
